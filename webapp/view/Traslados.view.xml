<mvc:View controllerName="morixe.zmmreemplazodsi.controller.Traslados"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns:form="sap.ui.layout.form"
    xmlns="sap.m">


    <Page title="{i18n>ttmovimiento}" class="Title PanelSnow">
        <customHeader>
            <Bar class="custHeader" design="Header">
                <contentLeft>
                    <Button type="Emphasized" icon="sap-icon://nav-back" press="_onGotoMainMenu"/>
                </contentLeft>
                <contentMiddle>
                    <ObjectIdentifier />
                </contentMiddle>
                <contentRight>
                    <!-- <Button id="idBtnReinit" visible="false" type="Emphasized" icon="sap-icon://refresh" press="onReiniciar"/> -->
                    <Image src="./images/logo.png" height="36px"></Image>
                </contentRight>
            </Bar>
        </customHeader>
        <content>
            <OverflowToolbar class="sapUiSmallMarginBottom">
                <ToolbarSpacer/>
                <Label text="{i18n>btntraslados}"/>
                <ToolbarSpacer/>
            </OverflowToolbar>
            <Toolbar >
                <content>
                    <Text text="Mensajes"/>
                    <ToolbarSpacer/>
                    <Button text="1" type="Emphasized" press="_onShowMsg1" class="sapUiTinyMarginBegin"/>
                    <Button text="2" type="Emphasized" press="_onShowMsg2" class="sapUiTinyMarginBegin"/>
                    <Button text="3" type="Emphasized" press="_onShowMsg3" class="sapUiTinyMarginBegin"/>
                    <Button text="4" type="Emphasized" press="_onShowMsg4" class="sapUiTinyMarginBegin"/>
                    <Button text="5" type="Emphasized" press="_onShowMsg5" class="sapUiTinyMarginBegin"/>
                    <Button text="6" type="Emphasized" press="_onShowMsg6" class="sapUiTinyMarginBegin"/>
                    <Button text="7" type="Emphasized" press="_onShowMsg7" class="sapUiTinyMarginBegin"/>
                    <Button text="8" type="Emphasized" press="_onShowMsg8" class="sapUiTinyMarginBegin"/>
                    <Button text="9" type="Emphasized" press="_onShowMsg9" class="sapUiTinyMarginBegin"/>
                    <Button text="10" type="Emphasized" press="_onShowMsg10" class="sapUiTinyMarginBegin"/>
                    <Button text="11" type="Emphasized" press="_onShowMsg11" class="sapUiTinyMarginBegin"/>


                </content>
            </Toolbar>


            <form:SimpleForm columnsM="2" columnsXL="4" editable="true" backgroundDesign="Transparent" visible="{= ${mockdata>/Etiquetado} === true }" class="sapUiTinyMarginTop">
                <form:content>


                    <Label text="{i18n>labelpallet}" labelFor="idMovPalletInput" />
                    <Input id="idMovPalletInput" submit="onPalletScan" />


                    <Text text="{i18n>ttdescmaterialplt}" class="cursiva" />
                </form:content>
            </form:SimpleForm>



            <form:SimpleForm visible="{= ${mockdata>/Etiquetado} === false }" columnsM="4" columnsXL="4" editable="true" backgroundDesign="Transparent" class="sapUiTinyMarginTop">
                <form:content>
                    <Label text="{i18n>lblmaterial}" />
                    <Input id="idTraPalletMaterial" submit="onPalletScan" showValueHelp="true" valueHelpIconSrc="sap-icon://bar-code"/>

                    <Label text="{i18n>lblcantidad}" />
                    <Input id="idTraPalletCantidad" submit="onPalletScan" />

                </form:content>
            </form:SimpleForm>

            <form:SimpleForm columnsM="2" columnsXL="4" editable="true" backgroundDesign="Transparent">
                <form:content>

                    <ObjectStatus title="{i18n>lblnumot}" text="XXXXXXXXXXXXXXX" />

                    <ObjectStatus title="{i18n>lbletapa}" text="XXX" />
                </form:content>
            </form:SimpleForm>

            <form:SimpleForm editable="true" backgroundDesign="Transparent" class="sapUiTinyMarginTop">
                <form:content>

                    <ObjectStatus title="{i18n>lblorigen}" text="XXXXXXXXXXXXXX" />
                    <Input submit="onInputScanSubmit" value="{mockdata>/Salida/Svalorigen}"/>
                </form:content>
            </form:SimpleForm>

            <form:SimpleForm editable="true" backgroundDesign="Transparent" class="sapUiTinyMarginTop">
                <form:content>
                    <ObjectStatus title="{i18n>lbldestino}" text="XXXXXXXXXXXXXXXXX" />
                    <Input submit="onInputScanSubmit" value="{mockdata>/Salida/Svaldestino}"/>

                    <Text text="{i18n>ttdesctipoalmacen}" class="cursiva" />


                </form:content>
            </form:SimpleForm>


        </content>
        <footer>

            <OverflowToolbar class="sapUiSmallMarginBottom">

                <Button text="{i18n>btncancelar}" type="Emphasized" width="22%" enabled="{mockdata>/inOTValidate}" press="_onGotoMainMenu"/>

                    <ToolbarSpacer/>
                <Button text="{i18n>btnconetiqueta}" press="onEtiquetado" type="Emphasized" width="22%" enabled="{mockdata>/inOTValidate}" visible="{= ${mockdata>/Etiquetado} === false }"/>
                <Button text="{i18n>btnsinetiqueta}" press="onEtiquetado" type="Emphasized" width="22%" enabled="{mockdata>/inOTValidate}" visible="{= ${mockdata>/Etiquetado} === true }"/>
                <Button text="{i18n>btnbuscar}" press="onEtiquetado" type="Emphasized" width="22%" enabled="{mockdata>/inOTValidate}" visible="{= ${mockdata>/Etiquetado} === true }"/>
                <ToolbarSpacer/>
                <Button text="{i18n>btncontinuar}" type="Emphasized" press="onAgregarSeleccionButtonPress" width="22%"/>

            </OverflowToolbar>
        </footer>
    </Page>

</mvc:View>