<mvc:View controllerName="morixe.zmmreemplazodsi.controller.Traslados"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns:form="sap.ui.layout.form"
    xmlns="sap.m">


    <Page title="{i18n>ttmovimiento}" class="Title PanelSnow">
        <customHeader>
            <Bar class="custHeader" design="Header">
                <contentLeft>
                    <Button type="Emphasized" icon="sap-icon://nav-back" press="_onGotoMainMenu"/>
                </contentLeft>
                <contentMiddle>
                    <ObjectIdentifier />
                </contentMiddle>
                <contentRight>
                    <!-- <Button id="idBtnReinit" visible="false" type="Emphasized" icon="sap-icon://refresh" press="onReiniciar"/> -->
                    <Image src="./images/logo.png" height="36px"></Image>
                </contentRight>
            </Bar>
        </customHeader>
        <content>
            <OverflowToolbar class="sapUiSmallMarginBottom">
                <ToolbarSpacer/>
                <Label text="{i18n>btntraslados}"/>
                <ToolbarSpacer/>
            </OverflowToolbar>
            <Toolbar visible="false">
                <content>
                    <Text text="Mensajes"/>
                    <ToolbarSpacer/>
                    <Button text="1" type="Emphasized" press="_onShowMsg1" class="sapUiTinyMarginBegin"/>
                    <Button text="2" type="Emphasized" press="_onShowMsg2" class="sapUiTinyMarginBegin"/>
                    <Button text="3" type="Emphasized" press="_onShowMsg3" class="sapUiTinyMarginBegin"/>
                    <Button text="4" type="Emphasized" press="_onShowMsg4" class="sapUiTinyMarginBegin"/>
                    <Button text="5" type="Emphasized" press="_onShowMsg5" class="sapUiTinyMarginBegin"/>
                    <Button text="6" type="Emphasized" press="_onShowMsg6" class="sapUiTinyMarginBegin"/>
                    <Button text="7" type="Emphasized" press="_onShowMsg7" class="sapUiTinyMarginBegin"/>
                    <Button text="8" type="Emphasized" press="_onShowMsg8" class="sapUiTinyMarginBegin"/>
                    <Button text="9" type="Emphasized" press="_onShowMsg9" class="sapUiTinyMarginBegin"/>
                    <Button text="10" type="Emphasized" press="_onShowMsg10" class="sapUiTinyMarginBegin"/>
                    <Button text="11" type="Emphasized" press="_onShowMsg11" class="sapUiTinyMarginBegin"/>


                </content>
            </Toolbar>


            <form:Form editable="true">
                <form:layout>
                    <form:ColumnLayout  columnsL="1" columnsXL="1" labelCellsLarge="1"/>
                </form:layout>
                <form:formContainers>
        
                    <form:FormContainer>
                        <form:formElements>
        
                            <form:FormElement label="{i18n>labelpallet}" visible="{= ${mockdata>/Etiquetado} === true }" >
                                <form:fields>
                                    <Input id="idMovPalletInput" submit="onPalletScan" value="{mockdata>/Traslado/Tpalletcodigo}"/>
                                </form:fields>
                            </form:FormElement>
                            <!-- Fin Elemento -->
        
                            <form:FormElement  visible="{= ${mockdata>/Etiquetado} === true }" >
                                <form:fields>
                                    <Text text="{mockdata>/Traslado/Tmaterialdesc}" class="cursiva" />
                                </form:fields>
                            </form:FormElement>
                            <!-- Fin Elemento -->
        
                            <form:FormElement  label="{i18n>lblmaterial}" visible="{= ${mockdata>/Etiquetado} === false }">
                                <form:fields>
                                    <Input id="idTraPalletMaterial" submit="onPalletScan" value="{mockdata>/Traslado/Tmaterialcodigo}" valueHelpIconSrc="sap-icon://bar-code"/>
                                </form:fields>
                            </form:FormElement>
                            <!-- Fin Elemento -->
        
                            <form:FormElement label="{i18n>lblcantidad}" visible="{= ${mockdata>/Etiquetado} === false }">
                                <form:fields>
                                    <Input id="idTraPalletCantidad" submit="onPalletScan" value="{mockdata>/Traslado/Tcantidad}"/>
                                </form:fields>
                            </form:FormElement>
                            <!-- Fin Elemento -->
        
                            <form:FormElement >
                                <form:fields>
                                    <ObjectStatus title="{i18n>lblnumot}" text="{mockdata>/Traslado/Totnumero}" />
        
                                        <ObjectStatus title="{i18n>lbletapa}" text="{mockdata>/Traslado/Tetapa}" />
                                </form:fields>
                            </form:FormElement>
                            <!-- Fin Elemento -->
        
        
                         
        
                            <form:FormElement >
                                <form:fields>
                                    <ObjectStatus title="{i18n>lblorigen}" text="{mockdata>/Traslado/Torigen}" />
                                </form:fields>
                            </form:FormElement>
                            <!-- Fin Elemento -->
        
        
                            <form:FormElement >
                                <form:fields>
                                    <Input submit="onInputScanSubmit" value="{mockdata>/Traslado/Tvalorigen}"/>
                                </form:fields>
                            </form:FormElement>
                            <!-- Fin Elemento -->
        
                            <form:FormElement >
                                <form:fields>
                                    <ObjectStatus title="{i18n>lbldestino}" text="{mockdata>/Traslado/Tdestino}" />
                                </form:fields>
                            </form:FormElement>
                            <!-- Fin Elemento -->
        
                            <form:FormElement >
                                <form:fields>
                                    <Input submit="onInputScanSubmit" value="{mockdata>/Traslado/Tvaldestino}"/>
                                </form:fields>
                            </form:FormElement>
                            <!-- Fin Elemento -->
        
                            <form:FormElement >
                                <form:fields>
                                    <Text text="{mockdata>/Traslado/Talmacendesc}" class="cursiva" />
                                </form:fields>
                            </form:FormElement>
                            <!-- Fin Elemento -->
        
        
        
                        </form:formElements>
        
                    </form:FormContainer>
        
                    <!-- Fin de do Contenedor -->
        
                </form:formContainers>
            </form:Form>


        </content>
        <footer>

            <OverflowToolbar class="sapUiSmallMarginBottom">

                <Button text="{i18n>btncancelar}" type="Emphasized" width="22%" enabled="{mockdata>/inOTValidate}" press="_onGotoMainMenu"/>

                <ToolbarSpacer/>
                <Button text="{i18n>btnconetiqueta}" press="onEtiquetado" type="Emphasized" width="22%" enabled="{mockdata>/inOTValidate}" visible="{= ${mockdata>/Etiquetado} === false }"/>
                <Button text="{i18n>btnsinetiqueta}" press="onEtiquetado" type="Emphasized" width="22%" enabled="{mockdata>/inOTValidate}" visible="{= ${mockdata>/Etiquetado} === true }"/>
                <Button text="{i18n>btnbuscar}" press="onEtiquetado" type="Emphasized" width="22%" enabled="{mockdata>/inOTValidate}" visible="{= ${mockdata>/Etiquetado} === true }"/>
                <ToolbarSpacer/>
                <Button text="{i18n>btncontinuar}" type="Emphasized" press="onTrasladoContinuar" width="22%"/>

            </OverflowToolbar>
        </footer>
    </Page>

</mvc:View>