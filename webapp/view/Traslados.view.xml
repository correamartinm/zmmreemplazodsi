<mvc:View controllerName="morixe.zmmreemplazodsi.controller.Traslados"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns:form="sap.ui.layout.form"
    xmlns="sap.m">


    <Page title="{i18n>ttmovimiento}" class="Title PanelSnow">
        <customHeader>
            <Bar class="custHeader" design="Header">
                <contentLeft>
                    <Button type="Critical" icon="sap-icon://nav-back" press="_onGotoMainMenu"/>
                </contentLeft>
                <contentMiddle>
                    <ObjectIdentifier />
                </contentMiddle>
                <contentRight>
                    <!-- <Button id="idBtnReinit" visible="false" type="Emphasized" icon="sap-icon://refresh" press="onReiniciar"/> -->
                    <Image src="./images/logo.png" height="36px"></Image>
                </contentRight>
            </Bar>
        </customHeader>
        <content>
            <OverflowToolbar class="sapUiSmallMarginBottom">
                <ToolbarSpacer/>
                <Label text="{i18n>btntraslados}"/>
                <ToolbarSpacer/>
            </OverflowToolbar>
            <Toolbar visible="false">
                <content>
                    <Text text="Mensajes"/>
                    <ToolbarSpacer/>
                    <Button text="1" type="Emphasized" press="_onShowMsg1" class="sapUiTinyMarginBegin"/>
                    <Button text="2" type="Emphasized" press="_onShowMsg2" class="sapUiTinyMarginBegin"/>
                    <Button text="3" type="Emphasized" press="_onShowMsg3" class="sapUiTinyMarginBegin"/>
                    <Button text="4" type="Emphasized" press="_onShowMsg4" class="sapUiTinyMarginBegin"/>
                    <Button text="5" type="Emphasized" press="_onShowMsg5" class="sapUiTinyMarginBegin"/>
                    <Button text="6" type="Emphasized" press="_onShowMsg6" class="sapUiTinyMarginBegin"/>
                    <Button text="7" type="Emphasized" press="_onShowMsg7" class="sapUiTinyMarginBegin"/>
                    <Button text="8" type="Emphasized" press="_onShowMsg8" class="sapUiTinyMarginBegin"/>
                    <Button text="9" type="Emphasized" press="_onShowMsg9" class="sapUiTinyMarginBegin"/>
                    <Button text="10" type="Emphasized" press="_onShowMsg10" class="sapUiTinyMarginBegin"/>
                    <Button text="11" type="Emphasized" press="_onShowMsg11" class="sapUiTinyMarginBegin"/>


                </content>
            </Toolbar>


            <form:Form editable="true">
                <form:layout>
                    <form:ColumnLayout columnsL="1" columnsXL="1" labelCellsLarge="1"/>
                </form:layout>
                <form:formContainers>

                    <form:FormContainer>
                        <form:formElements>

                            <form:FormElement label="{i18n>labelpallet}" visible="{= ${mockdata>/Etiquetado} === true }">
                                <form:fields>
                                    <Input id="idMovPalletInput" submit="onInputPalleScanSubmit" value="{mockdata>/TrasladoScan/Pallet}"/>
                                </form:fields>
                            </form:FormElement>
                            <!-- Fin Elemento -->

                            <form:FormElement visible="{= ${mockdata>/Etiquetado} === true }">
                                <form:fields>
                                    <Text text="{mockdata>/Traslado/DescripcionMaterial}" class="cursiva" />
                                </form:fields>
                            </form:FormElement>
                            <!-- Fin Elemento -->

                            <form:FormElement visible="{= ${mockdata>/Etiquetado} === false }">
                                <form:fields>
                                    <ObjectStatus title="{i18n>lblmaterial}" text="{mockdata>/Traslado/Material} - {mockdata>/Traslado/DescripcionMaterial}" class="uptitle" />
                                </form:fields>
                            </form:FormElement>

                            <form:FormElement visible="{= ${mockdata>/Etiquetado} === false }">
                                <form:fields>
                                    <Input enabled="{= ${mockdata>/Traslado/Cantidad} !== {mockdata>/TrasladoScan/Cantidad} }" id="idTraPalletMaterial" submit="onMaterialScan" value="{mockdata>/TrasladoScan/Material}" valueHelpIconSrc="sap-icon://bar-code"/>
                                </form:fields>
                            </form:FormElement>
                            <!-- Fin Elemento -->

                            <form:FormElement visible="{= ${mockdata>/Etiquetado} === false }">
                                <form:fields>
                                    <ObjectStatus title="{i18n>lblcantidad}" text="{mockdata>/Traslado/Cantidad}" class="uptitle" />
                                </form:fields>
                            </form:FormElement>

                            <form:FormElement visible="{= ${mockdata>/Etiquetado} === false }">
                                <form:fields>
                                    <Input enabled="{= ${mockdata>/Traslado/Cantidad} !== {mockdata>/TrasladoScan/Cantidad} }" id="idTraPalletCantidad" submit="onCantidadScan" value="{mockdata>/TrasladoScan/Cantidad}"/>
                                </form:fields>
                            </form:FormElement>
                            <!-- Fin Elemento -->

                            <form:FormElement >
                                <form:fields>
                                    <ObjectStatus title="{i18n>lblnumot}" text="{mockdata>/Traslado/Ot}" />

                                    <ObjectStatus title="{i18n>lbletapa}" text="{mockdata>/Traslado/Etapa}" />
                                </form:fields>
                            </form:FormElement>
                            <!-- Fin Elemento -->




                            <form:FormElement >
                                <form:fields>
                                    <ObjectStatus title="{i18n>lblorigen}" text="{mockdata>/Traslado/Origen}" class="uptitle"/>
                                </form:fields>
                            </form:FormElement>
                            <!-- Fin Elemento -->


                            <form:FormElement >
                                <form:fields>
                                    <Input submit="onInputOrigenSubmit" id="idTraOrigen" value="{mockdata>/TrasladoScan/Origen}"/>
                                </form:fields>
                            </form:FormElement>
                            <!-- Fin Elemento -->

                            <form:FormElement >
                                <form:fields>
                                    <ObjectStatus title="{i18n>lbldestino}" text="{mockdata>/Traslado/Destino}" class="uptitle" />
                                </form:fields>
                            </form:FormElement>
                            <!-- Fin Elemento -->

                            <form:FormElement >
                                <form:fields>
                                    <Input submit="onInputDestinoSubmit"  id="idTraDestino" value="{mockdata>/TrasladoScan/Destino}"/>
                                </form:fields>
                            </form:FormElement>
                            <!-- Fin Elemento -->

                            <form:FormElement >
                                <form:fields>
                                    <Text text="{mockdata>/Traslado/DescripcionAlmacen}" class="cursiva" />
                                </form:fields>
                            </form:FormElement>
                            <!-- Fin Elemento -->



                        </form:formElements>

                    </form:FormContainer>

                    <!-- Fin de do Contenedor -->

                </form:formContainers>
            </form:Form>


        </content>
        <footer>

            <OverflowToolbar class="sapUiSmallMarginBottom">

                <Button type="Critical" width="12%" icon="sap-icon://sys-cancel" press="_onGotoMainMenu"/>
                <!-- text="{i18n>btncancelar}" -->

                <ToolbarSpacer/>
                <Button text="{i18n>btnconetiqueta}" press="onEtiquetado" type="Emphasized" width="28%" enabled="{mockdata>/inOTValidate}" visible="{= ${mockdata>/Etiquetado} === false }"/>
                <Button text="{i18n>btnsinetiqueta}" press="onEtiquetado" type="Emphasized" width="28%" enabled="{mockdata>/inOTValidate}" visible="{= ${mockdata>/Etiquetado} === true }"/>
                <Button text="{i18n>btnbuscar}" press="onBuscarOt" type="Emphasized" width="22%" enabled="{mockdata>/inOTValidate}" visible="{= ${mockdata>/Etiquetado} === true }"/>

                <Button text="{i18n>btncontinuar}" type="Emphasized" press="onTrasladoContinuar" width="23%" visible="{= ${mockdata>/Etiquetado} === false }" enabled="{mockdata>/AlmValidTraslado}"/>
                <Button text="{i18n>btncontinuar}" type="Emphasized" press="onTrasladoSinEtiquetaContinuar" width="23%" visible="{= ${mockdata>/Etiquetado} === true }" enabled="{mockdata>/AlmValidTrasladoSinEtiqueta}"/>

            </OverflowToolbar>
        </footer>
    </Page>

</mvc:View>