<mvc:View controllerName="morixe.zmmreemplazodsi.controller.Salidas"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns:form="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
    xmlns:card="sap.f.cards"
    xmlns:f="sap.f"
    xmlns="sap.m">


    <Page title="{i18n>ttotsaliente}" id="OtSaliente" class="Title PanelSnow">
        <customHeader>
            <Bar class="custHeader" design="Header">
                <contentLeft>
                    <Button type="Emphasized" icon="sap-icon://nav-back" press="_onGotoMainMenu"/>

                </contentLeft>
                <contentMiddle>
                    <ObjectIdentifier />
                </contentMiddle>
                <contentRight>

                    <Image src="./images/logo.png" height="36px"></Image>
                </contentRight>
            </Bar>
        </customHeader>
        <content>

            <OverflowToolbar >
                <ToolbarSpacer/>
                <Label text="{i18n>btnsalidaventas}" />
                <ToolbarSpacer/>
            </OverflowToolbar>

            <Toolbar >
                <content>
                    <Text text="Mensajes"/>
                    <ToolbarSpacer/>
                    <Button text="1" type="Emphasized" press="_onShowMsg1" class="sapUiTinyMarginBegin"/>
                    <Button text="2" type="Emphasized" press="_onShowMsg2" class="sapUiTinyMarginBegin"/>
                    <Button text="3" type="Emphasized" press="_onShowMsg3" class="sapUiTinyMarginBegin"/>
                    <Button text="4" type="Emphasized" press="_onShowMsg4" class="sapUiTinyMarginBegin"/>
                    <Button text="5" type="Emphasized" press="_onShowMsg5" class="sapUiTinyMarginBegin"/>
                    <Button text="6" type="Emphasized" press="_onShowMsg6" class="sapUiTinyMarginBegin"/>
                    <Button text="7" type="Emphasized" press="_onShowMsg7" class="sapUiTinyMarginBegin"/>
                    <Button text="8" type="Emphasized" press="_onShowMsg8" class="sapUiTinyMarginBegin"/>
                    <Button text="9" type="Emphasized" press="_onShowMsg9" class="sapUiTinyMarginBegin"/>
                </content>
            </Toolbar>



            <HBox alignContent="Center" alignItems="Center" justifyContent="End" class="sapUiTinyMarginTop sapUiTinyMarginEnd">

                <Label text="{i18n>lblposicion}: " />
                <Label text="1/X" class="BigSize sapUiTinyMarginBegin"/>
            </HBox>
            
            <form:Form editable="true">
                <form:layout>
                    <form:ColumnLayout columnsM="1" columnsL="1" columnsXL="1" labelCellsLarge="2"/>
                </form:layout>
                <form:formContainers>
        
                    <form:FormContainer>
                        <form:formElements>
        
                            <form:FormElement label="{i18n>lblingreseot}">
                                <form:fields>
                                    <Input id="idOtOutInput" submit="onInputOtScanSubmit" />
                                </form:fields>
                            </form:FormElement>
                            <!-- Fin Elemento -->
        
                            <form:FormElement >
                                <form:fields>
                                    <ObjectStatus title="{i18n>lbletapa}" text="XXX" />
                                </form:fields>
                            </form:FormElement>
                            <!-- Fin Elemento -->
        
                        
                            <form:FormElement >
                                <form:fields>
                                    <ObjectStatus title="{i18n>lblorigen}" text="XXXXXXXXXXX" />
                            
                                </form:fields>
                            </form:FormElement>
                            <!-- Fin Elemento -->

                            <form:FormElement >
                                <form:fields>
                            
                                    <Input submit="onInputScanSubmit" value="{mockdata>/Salida/Svalorigen}"/>
                                </form:fields>
                            </form:FormElement>
                            <!-- Fin Elemento -->
        
                            <form:FormElement  label="{i18n>labelpallet}">
                                <form:fields>
                                    <Input valueStateText="{i18n>msgdiferente}" name="inCodigo" id="idCodigoInput" submit="onInputScanSubmit" value="{mockdata>/Salida/Svalpalletcodigo}" />
                                </form:fields>
                            </form:FormElement>
                            <!-- Fin Elemento -->
                            
        
                            <form:FormElement  >
                                <form:fields>
                                    <ObjectStatus title="{i18n>lbldestino}" text="XXXXXXXXXXX" />
                                    
                                </form:fields>
                            </form:FormElement>
                            <!-- Fin Elemento -->

                            <form:FormElement  >
                                <form:fields>
                                    
                                    <Input submit="onInputScanSubmit" value="{mockdata>/Salida/Svalorigen}"/>  
                                </form:fields>
                            </form:FormElement>
                            <!-- Fin Elemento -->
        
        
        
        
        
                        </form:formElements>
        
                    </form:FormContainer>
        
                    <!-- Fin de do Contenedor -->
        
                </form:formContainers>
            </form:Form>



            <HBox class="sapUiLargeMarginTop" justifyContent="SpaceAround" alignContent="Center" alignItems="Center">

                <VBox alignContent="Center" alignItems="Center">
                    <core:Icon src="sap-icon://navigation-left-arrow" color="blue" class="size2" press=""/>
                    <Label text="{i18n>btnanterior}" />
                </VBox>

                <VBox alignContent="Center" alignItems="Center">

                    <Button text="{i18n>btnleerpallet}" type="Default"/>

                </VBox>

                <VBox alignContent="Center" alignItems="Center">
                    <core:Icon src="sap-icon://navigation-right-arrow" color="blue" class="size2" press=""/>
                    <Label text="{i18n>btnsiguiente}" />

                </VBox>


            </HBox>


        </content>
        <footer>


            <OverflowToolbar class="sapUiSmallMarginBottom">

                <Button text="{i18n>btncancelar}" type="Emphasized" width="22%" enabled="{mockdata>/inOTValidate}" press="_onGotoMainMenu"/>
                    <ToolbarSpacer/>
                <Button text="{i18n>btnlimpiar}" type="Emphasized" width="22%" enabled="{mockdata>/inOTValidate}"/>
                <Button text="{i18n>btnbuscar}" type="Emphasized" width="22%" enabled="{mockdata>/inOTValidate}"/>
                <ToolbarSpacer/>
                <Button text="{i18n>btncontinuar}" type="Emphasized" width="22%" />
            </OverflowToolbar>

        </footer>
    </Page>

</mvc:View>
