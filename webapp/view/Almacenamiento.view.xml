<mvc:View controllerName="morixe.zmmreemplazodsi.controller.Almacenamiento"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns:form="sap.ui.layout.form"
    xmlns="sap.m">


    <Page title="{i18n>ttotentrante}" class="Title PanelSnow">
        <customHeader>
            <Bar class="custHeader" design="Header">
                <contentLeft>
                    <Button type="Emphasized" icon="sap-icon://nav-back" press="_onGotoMainMenu"/>

                </contentLeft>
                <contentMiddle>
                    <ObjectIdentifier />
                </contentMiddle>
                <contentRight>

                    <Image src="./images/logo.png" height="36px"></Image>
                </contentRight>
            </Bar>
        </customHeader>
        <content>
            <OverflowToolbar >
                <ToolbarSpacer/>
                <Label text="{i18n>lblalmacenamiento}"/>
                <ToolbarSpacer/>
            </OverflowToolbar>



            <VBox visible="{= ${mockdata>/EtiquIngxPallets} === false }">


                <form:Form editable="true">
                    <form:layout>
                        <form:ColumnLayout columnsL="1" columnsXL="1" labelCellsLarge="1"/>
                    </form:layout>
                    <form:formContainers>

                        <form:FormContainer>
                            <form:formElements>

                                <form:FormElement label="{i18n>lblingresepallet}">
                                    <form:fields>
                                        <Input name="inPallet" id="idAlmPalletScan" submit="onInputPalleScanSubmit" value="{mockdata>/AlmacenamientoScan/Pallet}" type="Number" />

                                    </form:fields>

                                </form:FormElement>
                                <form:FormElement  >
                                    <form:fields>
                                        <Text text="{mockdata>/Almacenamiento/Descripcion}" class="cursiva" />

                                    </form:fields>
                                </form:FormElement>
                                <!-- Fin Elemento -->

                                <form:FormElement>
                                    <form:fields>
                                        <ObjectStatus title="{i18n>lblnumot}" text="{mockdata>/Almacenamiento/Ot}" />
                                        <ObjectStatus title="{i18n>lbletapa}" text="{mockdata>/Almacenamiento/Etapa}" />

                                    </form:fields>
                                </form:FormElement>
                                <form:FormElement  >
                                    <form:fields>

                                        <Text text="{mockdata>/inOTAlmmacen}"/>

                                    </form:fields>
                                </form:FormElement>
                                <!-- Fin Elemento -->

                                <form:FormElement >
                                    <form:fields>
                                        <ObjectStatus title="{i18n>lbldestino}" text="{mockdata>/Almacenamiento/Destino}" />

                                    </form:fields>
                                </form:FormElement>
                                <form:FormElement  >
                                    <form:fields>

                                        <Input id="idAlmDestinoScan" submit="onInputDestinoSubmit" type="Text" value="{mockdata>/AlmacenamientoScan/Destino}"/>



                                    </form:fields>
                                </form:FormElement>

                                <form:FormElement  >
                                    <form:fields>

                                        <Text text="{mockdata>/Almacenamiento/DescripcionAlmacen}" class="cursiva" />


                                    </form:fields>
                                </form:FormElement>
                                <!-- Fin Elemento -->




                            </form:formElements>

                        </form:FormContainer>

                        <!-- Fin de do Contenedor -->

                    </form:formContainers>
                </form:Form>
            </VBox>

            <!-- ***************************** -->
            <VBox visible="{= ${mockdata>/EtiquIngxPallets} === true }">

                <form:Form editable="true">
                    <form:layout>
                        <form:ColumnLayout columnsL="1" />
                    </form:layout>
                    <form:formContainers>

                        <form:FormContainer>
                            <form:formElements>
                                <form:FormElement >
                                    <form:fields>
                                        <ObjectStatus title="{i18n>lblmaterial}" text="{mockdata>/Devolucion/Material} - {mockdata>/Devolucion/DescripcionMaterial}" class="uptitle" />
                                    </form:fields>
                                </form:FormElement>

                                <form:FormElement >
                                    <form:fields>
                                        <Input id="idAlmDevPalletMaterial" submit="onMaterialScan" value="{mockdata>/DevolucionScan/Material}" showValueHelp="true" valueHelpIconSrc="sap-icon://bar-code" />
                                    </form:fields>
                                </form:FormElement>

                                <!-- Fin Elemento -->
                                <form:FormElement >
                                    <form:fields>
                                        <ObjectStatus title="{i18n>lblcantidad}" text="{mockdata>/Devolucion/Cantidad}" class="uptitle" />
                                    </form:fields>
                                </form:FormElement>


                                <form:FormElement >
                                    <form:fields>
                                        <Input id="idAlmDevPalletCantidad" submit="onCantidadScan" value="{mockdata>/DevolucionScan/Cantidad}"/>
                                    </form:fields>
                                </form:FormElement>
                                <!-- Fin Elemento -->

                                <form:FormElement >
                                    <form:fields>
                                        <ObjectStatus title="{i18n>lblnumot}" text="{mockdata>/Devolucion/Ot}" />

                                        <ObjectStatus title="{i18n>lbletapa}" text="{mockdata>/Devolucion/Etapa}" />
                                    </form:fields>
                                </form:FormElement>
                                <!-- Fin Elemento -->

                                <form:FormElement >
                                    <form:fields>
                                        <ObjectStatus title="{i18n>lblorigen}" text="{mockdata>/Devolucion/Origen}" class="uptitle" />
                                    </form:fields>
                                </form:FormElement>
                                <!-- Fin Elemento -->

                                <form:FormElement >
                                    <form:fields>
                                        <ObjectStatus title="{i18n>lbldestino}" text="{mockdata>/Devolucion/Destino} - {mockdata>/Devolucion/DescripcionAlmacen}" class="uptitle" />
                                    </form:fields>
                                </form:FormElement>
                                <!-- Fin Elemento -->

                                <form:FormElement >
                                    <form:fields>
                                        <Input id="idAlmDevDestinoScan" submit="onDestinoScan" value="{mockdata>/DevolucionScan/Destino}"/>
                                    </form:fields>
                                </form:FormElement>
                                <!-- Fin Elemento -->
                                <form:FormElement >
                                    <form:fields>
                                        <Text text="{mockdata>/Devolucion/DescripcionAlmacen}" class="cursiva" />
                                    </form:fields>
                                </form:FormElement>
                                <!-- Fin Elemento -->


                            </form:formElements>

                        </form:FormContainer>

                        <!-- Fin de do Contenedor -->

                    </form:formContainers>
                </form:Form>


            </VBox>



        </content>
        <footer>

            <OverflowToolbar class="sapUiSmallMarginBottom">
                <Button text="{i18n>btncancelar}" type="Emphasized" width="27%" enabled="{mockdata>/inOTValidate}" press="_onGotoMainMenu"/>
                <ToolbarSpacer/>
                <Button text="{i18n>btningpallet}" type="Emphasized" press="onIngresaxPallet" visible="{= ${mockdata>/EtiquIngxPallets} === true }"/>
                <Button text="{i18n>btnlimpiar}" type="Emphasized" press="onClearScreen" />
                <ToolbarSpacer/>
                <Button text="{i18n>btncontinuar}" tooltip="={mockdata>/AlmValidAlmacenamiento}" type="Emphasized" width="27%" enabled="{mockdata>/AlmValidAlmacenamiento}" press="onConfirmaAlmacenamiento" visible="{= ${mockdata>/EtiquIngxPallets} === false }"/>
                <Button text="{i18n>btncontinuar}" tooltip="={mockdata>/AlmValidDevolucion}" type="Emphasized" width="27%" enabled="{mockdata>/AlmValidDevolucion}" press="onConfirmaDevolucion" visible="{= ${mockdata>/EtiquIngxPallets} === true }"/>
            </OverflowToolbar>
        </footer>
    </Page>

</mvc:View>
