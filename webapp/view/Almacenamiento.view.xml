<mvc:View controllerName="morixe.zmmreemplazodsi.controller.Almacenamiento"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns:form="sap.ui.layout.form"
    xmlns="sap.m">


    <Page title="{i18n>ttotentrante}" class="Title PanelSnow">
        <customHeader>
            <Bar class="custHeader" design="Header">
                <contentLeft>
                    <Button type="Emphasized" icon="sap-icon://nav-back" press="_onGotoMainMenu"/>

                </contentLeft>
                <contentMiddle>
                    <ObjectIdentifier />
                </contentMiddle>
                <contentRight>

                    <Image src="./images/logo.png" height="36px"></Image>
                </contentRight>
            </Bar>
        </customHeader>
        <content>
            <OverflowToolbar >
                <ToolbarSpacer/>
                <Label text="{i18n>lblalmacenamiento}"/>
                <ToolbarSpacer/>
            </OverflowToolbar>
           


            <VBox visible="{= ${mockdata>/EtiquIngxPallets} === false }">


                <form:Form editable="true">
                    <form:layout>
                        <form:ColumnLayout  columnsL="1" columnsXL="1" labelCellsLarge="1"/>
                    </form:layout>
                    <form:formContainers>

                        <form:FormContainer>
                            <form:formElements>

                                <form:FormElement label="{i18n>lblingresepallet}">
                                    <form:fields>
                                        <Input name="inPallet" id="idOtInPalletInput" submit="onInputScanSubmit" value="{mockdata>/Almacenamiento/Pallet}" />

                                    </form:fields>

                                </form:FormElement>
                                <form:FormElement  >
                                    <form:fields>
                                        <Text text="{mockdata>/Almacenamiento/Descripcion}" class="cursiva" />

                                    </form:fields>
                                </form:FormElement>
                                <!-- Fin Elemento -->

                                <form:FormElement>
                                    <form:fields>
                                        <ObjectStatus title="{i18n>lblnumot}" text="{mockdata>/Almacenamiento/Ot}" />
                                        <ObjectStatus title="{i18n>lbletapa}" text="{mockdata>/Almacenamiento/Etapa}" />

                                    </form:fields>
                                </form:FormElement>
                                <form:FormElement  >
                                    <form:fields>

                                        <Text text="{mockdata>/inOTAlmmacen}"/>

                                    </form:fields>
                                </form:FormElement>
                                <!-- Fin Elemento -->

                                <form:FormElement >
                                    <form:fields>
                                        <ObjectStatus title="{i18n>lbldestino}" text="{mockdata>/Almacenamiento/Destino}" />

                                    </form:fields>
                                </form:FormElement>
                                <form:FormElement  >
                                    <form:fields>

                                        <Input submit="onInputDestinoSubmit" />



                                    </form:fields>
                                </form:FormElement>

                                <form:FormElement  >
                                    <form:fields>

                                        <Text text="{mockdata>/Almacenamiento/DescripcionAlmacen}" class="cursiva" />


                                    </form:fields>
                                </form:FormElement>
                                <!-- Fin Elemento -->




                            </form:formElements>

                        </form:FormContainer>

                        <!-- Fin de do Contenedor -->

                    </form:formContainers>
                </form:Form>
            </VBox>

            <!-- ***************************** -->
            <VBox visible="{= ${mockdata>/EtiquIngxPallets} === true }">

                <form:Form editable="true">
                    <form:layout>
                        <form:ColumnLayout  columnsL="1" />
                    </form:layout>
                    <form:formContainers>

                        <form:FormContainer>
                            <form:formElements>

                                <form:FormElement label="{i18n>lblmaterial}">
                                    <form:fields>
                                        <Input id="idAlmPalletMaterial" submit="onPalletScan" showValueHelp="true" valueHelpIconSrc="sap-icon://bar-code"/>
                                        
                                     
                                    </form:fields>
                                </form:FormElement>
                                <!-- Fin Elemento -->

                                <form:FormElement label="{i18n>lblcantidad}">
                                    <form:fields>
                                        <Input id="idAlmPalletCantidad" submit="onPalletScan" />
                                    </form:fields>
                                </form:FormElement>
                                <!-- Fin Elemento -->

                                <form:FormElement >
                                    <form:fields>
                                        <ObjectStatus title="{i18n>lblnumot}" text="XXXXXXXXXXXX" />

                                        <ObjectStatus title="{i18n>lbletapa}" text="XXX" />
                                    </form:fields>
                                </form:FormElement>
                                <!-- Fin Elemento -->

                                <form:FormElement >
                                    <form:fields>
                                        <ObjectStatus title="{i18n>lblorigen}" text="XXXXXXXXXXXXXX" />
                                    </form:fields>
                                </form:FormElement>
                                <!-- Fin Elemento -->

                                <form:FormElement >
                                    <form:fields>
                                        <ObjectStatus title="{i18n>lbldestino}" text="XXXXXXXXXXXXXXXXX" />
                                    </form:fields>
                                </form:FormElement>
                                <!-- Fin Elemento -->

                                <form:FormElement >
                                    <form:fields>
                                        <Input submit="onInputScanSubmit" value="{mockdata>/Salida/Svaldestino}"/>
                                    </form:fields>
                                </form:FormElement>
                                <!-- Fin Elemento -->
                                <form:FormElement >
                                    <form:fields>
                                        <Text text="{i18n>ttdesctipoalmacen}" class="cursiva" />
                                    </form:fields>
                                </form:FormElement>
                                <!-- Fin Elemento -->







                            </form:formElements>

                        </form:FormContainer>

                        <!-- Fin de do Contenedor -->

                    </form:formContainers>
                </form:Form>

               
            </VBox>



        </content>
        <footer>

            <OverflowToolbar class="sapUiSmallMarginBottom">
                <Button text="{i18n>btncancelar}" type="Emphasized" width="30%" enabled="{mockdata>/inOTValidate}" press="_onGotoMainMenu"/>
                <ToolbarSpacer/>
                <Button text="{i18n>btningpallet}" visible="{= ${mockdata>/EtiquIngxPallets} === false }" type="Emphasized" width="30%" press="onIngresaxPallet"/>
                <Button text="{i18n>btningpallet}" visible="{= ${mockdata>/EtiquIngxPallets} === true }" type="Accept" width="30%" press="onIngresaxPallet"/>
                <ToolbarSpacer/>
                <Button text="{i18n>btncontinuar}" type="Emphasized" width="30%" enabled="{mockdata>/inOTValidate}" press=""/>
            </OverflowToolbar>
        </footer>
    </Page>

</mvc:View>
